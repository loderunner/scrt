{
  "compilerOptions": {
    "target": "ESNext",
    "esModuleInterop": true,
    "types": ["@vuepress/client/types"],
    "jsx": "preserve",
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": [".vuepress/**/*.ts", ".vuepress/**/*.vue"],
  "exclude": [
    ".vuepress/dist/**/*.ts",
    ".vuepress/.cache/**/*.ts",
    ".vuepress/.temp/**/*.ts"
  ]
}
